# SECTION 1: LOAD DATA
print("\n--- Loading cleaned data ---")
df <- readRDS("energy_data_clean.rds")
print("Data loaded successfully!")
# SECTION 2: TWO-SAMPLE T-TEST - BUILDING HEIGHT (HEATING LOAD)
heating_3_5m <- df %>% filter(Height == "3.5m") %>% pull(Heating_Load)
heating_7_0m <- df %>% filter(Height == "7.0m") %>% pull(Heating_Load)
print("\n--- Descriptive Statistics ---")
print(sprintf("3.5m buildings: n = %d, Mean = %.2f, SD = %.2f",
length(heating_3_5m), mean(heating_3_5m), sd(heating_3_5m)))
print(sprintf("7.0m buildings: n = %d, Mean = %.2f, SD = %.2f",
length(heating_7_0m), mean(heating_7_0m), sd(heating_7_0m)))
levene_height_heating <- leveneTest(Heating_Load ~ Height, data = df)
print("\n--- Levene's Test for Equality of Variances ---")
print(levene_height_heating)
t_test_height_heating <- t.test(Heating_Load ~ Height, data = df, var.equal = TRUE)
print("\n--- Independent Two-Sample T-Test ---")
print(t_test_height_heating)
# SECTION 2: TWO-SAMPLE T-TEST - BUILDING HEIGHT
# Hypothesis 1: Does building height affect heating load?
# Separate data by height
heating_3_5m <- df %>% filter(Height == "3.5m") %>% pull(Heating_Load)
heating_7_0m <- df %>% filter(Height == "7.0m") %>% pull(Heating_Load)
print("Descriptive Statistics")
print(sprintf("3.5m buildings: n = %d, Mean = %.2f, SD = %.2f",
length(heating_3_5m), mean(heating_3_5m), sd(heating_3_5m)))
print(sprintf("7.0m buildings: n = %d, Mean = %.2f, SD = %.2f",
length(heating_7_0m), mean(heating_7_0m), sd(heating_7_0m)))
# Test for equal variances (Levene's test)
print("Levene's Test for Equality of Variances")
levene_height_heating <- leveneTest(Heating_Load ~ Height, data = df)
print(levene_height_heating)
# Conduct t-test
print("Independent Two-Sample T-Test")
t_test_height_heating <- t.test(Heating_Load ~ Height, data = df, var.equal = TRUE)
print(t_test_height_heating)
# SECTION 3: T-TEST - COOLING LOAD BY BUILDING HEIGHT
cooling_3_5m <- df %>% filter(Height == "3.5m") %>% pull(Cooling_Load)
cooling_7_0m <- df %>% filter(Height == "7.0m") %>% pull(Cooling_Load)
print(sprintf("3.5m buildings: n = %d, Mean = %.2f, SD = %.2f",
length(cooling_3_5m), mean(cooling_3_5m), sd(cooling_3_5m)))
print(sprintf("7.0m buildings: n = %d, Mean = %.2f, SD = %.2f",
length(cooling_7_0m), mean(cooling_7_0m), sd(cooling_7_0m)))
print("Independent Two-Sample T-Test")
t_test_height_cooling <- t.test(Cooling_Load ~ Height, data = df, var.equal = TRUE)
print(t_test_height_cooling)
# SECTION 4: ONE-WAY ANOVA - ORIENTATION
print("Descriptive Statistics by Orientation")
orientation_stats <- df %>%
group_by(Orientation) %>%
summarise(
n = n(),
Mean = mean(Heating_Load),
SD = sd(Heating_Load),
Min = min(Heating_Load),
Max = max(Heating_Load)
)
print(orientation_stats)
print("Levene's Test for Homogeneity of Variances")
levene_orient <- leveneTest(Heating_Load ~ Orientation, data = df)
print(levene_orient)
print("One-Way ANOVA")
anova_orientation <- aov(Heating_Load ~ Orientation, data = df)
print(summary(anova_orientation))
anova_p <- summary(anova_orientation)[[1]]$`Pr(>F)`[1]
if(anova_p < 0.05) {
print("Post-Hoc Test: Tukey HSD")
tukey_orient <- TukeyHSD(anova_orientation)
print(tukey_orient)
}
# SECTION 5: ONE-WAY ANOVA - GLAZING AREA
print("Descriptive Statistics by Glazing Area")
glazing_stats <- df %>%
group_by(Glazing_Area) %>%
summarise(
n = n(),
Mean = mean(Heating_Load),
SD = sd(Heating_Load),
Min = min(Heating_Load),
Max = max(Heating_Load)
)
