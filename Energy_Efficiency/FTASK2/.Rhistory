# SECTION 1: LOAD AND PREPARE DATA
print("Loading cleaned data")
df <- readRDS("energy_data_clean.rds")
print("Data loaded successfully")
df_numeric <- df %>%
mutate(
Height_num = as.numeric(Height) - 1,
Orientation_num = as.numeric(Orientation) - 1,
Glazing_Area_num = as.numeric(Glazing_Area) - 1,
Glazing_Dist_num = as.numeric(Glazing_Dist) - 1
)
print("Categorical variables converted to numeric")
# SECTION 2: SIMPLE LINEAR REGRESSION - HEATING LOAD
model1 <- lm(Heating_Load ~ Compactness, data = df_numeric)
print("Model 1: Heating Load ~ Compactness")
print(summary(model1))
r_squared_1 <- summary(model1)$r.squared
adj_r_squared_1 <- summary(model1)$adj.r.squared
rmse_1 <- sqrt(mean(model1$residuals^2))
print(paste("Model 1 Performance: R-squared =", round(r_squared_1, 4),
"Adjusted R-squared =", round(adj_r_squared_1, 4),
"RMSE =", round(rmse_1, 4)))
# SECTION 3: MULTIPLE LINEAR REGRESSION - HEATING LOAD
model2 <- lm(Heating_Load ~ Compactness + Surface_Area + Wall_Area + Roof_Area,
data = df_numeric)
print("Model 2: Heating Load ~ All Continuous Predictors")
print(summary(model2))
r_squared_2 <- summary(model2)$r.squared
adj_r_squared_2 <- summary(model2)$adj.r.squared
rmse_2 <- sqrt(mean(model2$residuals^2))
print(paste("Model 2 Performance: R-squared =", round(r_squared_2, 4),
"Adjusted R-squared =", round(adj_r_squared_2, 4),
"RMSE =", round(rmse_2, 4)))
vif_values <- vif(model2)
print("Variance Inflation Factors (VIF):")
print(vif_values)
# SECTION 4: FULL MODEL - HEATING LOAD (ALL VARIABLES)
model3 <- lm(Heating_Load ~ Compactness + Surface_Area + Wall_Area + Roof_Area +
Height_num + Orientation_num + Glazing_Area_num + Glazing_Dist_num,
data = df_numeric)
print("Model 3: Full Model (All Predictors)")
print(summary(model3))
r_squared_3 <- summary(model3)$r.squared
adj_r_squared_3 <- summary(model3)$adj.r.squared
rmse_3 <- sqrt(mean(model3$residuals^2))
print(paste("Model 3 Performance: R-squared =", round(r_squared_3, 4),
"Adjusted R-squared =", round(adj_r_squared_3, 4),
"RMSE =", round(rmse_3, 4)))
vif_full <- vif(model3)
print("VIF for Full Model:")
print(vif_full)
# SECTION 5: STEPWISE REGRESSION - HEATING LOAD
model4 <- stepAIC(model3, direction = "backward", trace = FALSE)
print("Model 4: Stepwise Regression")
print(summary(model4))
r_squared_4 <- summary(model4)$r.squared
adj_r_squared_4 <- summary(model4)$adj.r.squared
rmse_4 <- sqrt(mean(model4$residuals^2))
print(paste("Model 4 Performance: R-squared =", round(r_squared_4, 4),
"Adjusted R-squared =", round(adj_r_squared_4, 4),
"RMSE =", round(rmse_4, 4)))
print("Variables retained in final model:")
print(names(coef(model4))[-1])
# SECTION 6: REGRESSION FOR COOLING LOAD
model5 <- lm(Cooling_Load ~ Compactness, data = df_numeric)
print("Model 5: Cooling Load ~ Compactness")
print(summary(model5))
model6 <- lm(Cooling_Load ~ Compactness + Surface_Area + Wall_Area + Roof_Area,
data = df_numeric)
print("Model 6: Cooling Load ~ Continuous Predictors")
