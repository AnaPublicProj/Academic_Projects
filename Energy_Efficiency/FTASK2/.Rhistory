print(paste("Model 6 (Multiple): R² =", round(summary(model6)$r.squared, 4),
"RMSE =", round(sqrt(mean(model6$residuals^2)), 4)))
print(paste("Model 7 (Full): R² =", round(summary(model7)$r.squared, 4),
