print(levene_glazing)
print("One-Way ANOVA")
anova_glazing <- aov(Heating_Load ~ Glazing_Area, data = df)
print(summary(anova_glazing))
anova_glaz_p <- summary(anova_glazing)[[1]]$`Pr(>F)`[1]
if(anova_glaz_p < 0.05) {
print("Post-Hoc Test: Tukey HSD")
